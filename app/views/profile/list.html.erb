<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "List", profile_list_path  %></li>
      <li><%= link_to "Technical Info", profile_index_path %></li>
    </ul>
  </div>          

	
      <div class="actions-bar wat-cf">
        <div class="actions"></div>
 
          <div class="content">
            <h2 class="title">User Profiles</h2>

            <div class="inner">
              <ul class="list">
		        <% @users.each do |user| -%>
                <li>
                  <div class="left"><%= image_tag("web-app-theme/amd.jpg") + " ."  %></div>
                  <div class="item"><p>Name: <%= user.first %> <%= user.last %></p></div>
                  <div class="item"><p>City: <%= user.city %></p></div>
               	  <div class="item"><p>Description: <%= user.comments %></p></div>
                </li>
 				<% end %>
              </ul>
				
          </div>
        </div>
       
        <%= will_paginate @users  %>
        
      </div>

</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
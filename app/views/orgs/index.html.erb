<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", orgs_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_org_path %></li>
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> Orgs</h2>
    <div class="inner">
      <table class="table">
        <tr>             
          <th class="first">ID</th>
          <th>Organization</th>
          <th>Created at</th>
          <th>Created by</th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @orgs.each do |org| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td><%= org.id %></td>
          <td><%= link_to org.organization, org_path(org) %></td>
          <td><%= org.created_at %></td>
          <td><%= org.address %></td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", org_path(org) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_org_path(org) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", org_path(org), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
